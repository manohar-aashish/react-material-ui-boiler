{
  "name": "react-boiler",
  "version": "1.0.0",
  "description": "This is a react boiler and can be used as a template for react projects",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./build",
    "prebuild": "npm ci --only-prod",
    "build": "webpack --config webpack/webpack.config.js --env env=prod",
    "dev": "webpack serve --config webpack/webpack.config.js --env env=dev --port 3000 --profile --progress",
    "format": "prettier --write \"./**/*.{js,jsx,css,scss,json}\"",
    "lint": "eslint --fix \"./**/*.{js,jsx,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serveBuild": "serve ./build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manohar-aashish/react-material-ui-boiler.git"
  },
  "author": "Aashish Manohar Varlani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manohar-aashish/react-material-ui-boiler/issues"
  },
  "homepage": "https://github.com/manohar-aashish/react-material-ui-boiler#readme",
  "dependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "html-webpack-plugin": "^5.3.2",
    "install": "^0.13.0",
    "npm": "^7.19.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-material-ui-carousel": "^2.2.7",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "eslint": "^7.30.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.2",
    "react-refresh": "^0.10.0",
    "serve": "^12.0.0",
    "webpack-dev-server": "^3.11.2"
  }
}
